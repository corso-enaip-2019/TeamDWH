<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <script>

        $(document).ready(function () {
            var x = document.getElementsByClassName('flip-card');
            setTimeout(function () {
                // console.log(x.length);
                $('body').addClass('loaded');
                if (x.length === 0 && (window.location.search !== '?loaded=' + 1 && window.location.search !== '?loaded=' + 2 && window.location.search !== '?loaded=' + 3)) {
                    window.location.reload();
                    window.location.search = '?loaded=' + 1;
                }
                else if (x.length === 0 && (window.location.search === '?loaded=' + 1 && window.location.search !== '?loaded=' + 2 && window.location.search !== '?loaded=' + 3)) {
                    window.location.reload();
                    window.location.search = '?loaded=' + 2;
                }
                else if (x.length === 0 && (window.location.search === '?loaded=' + 2 && window.location.search !== '?loaded=' + 1 && window.location.search !== '?loaded=' + 3)) {
                    window.location.reload();
                    window.location.search = '?loaded=' + 3;
                }
            }, 2000);
        });

    </script>
    <title>Portale DWH</title>

    <base href="/">
</head>
<body>
    @RenderBody()

    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/runtime-es2015.js" type="module"></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/polyfills-es2015.js" type="module"></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/runtime-es5.js" nomodule></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/polyfills-es5.js" nomodule></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/styles-es2015.js" type="module"></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/styles-es5.js" nomodule></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/vendor-es2015.js" type="module"></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/main-es2015.js" type="module"></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/vendor-es5.js" nomodule></script>
    <script src="~/Dashboardtemplate/DashDemo/dist/DashDemo/main-es5.js" nomodule></script>

    @RenderSection("scripts", required: false)
</body>
</html>
